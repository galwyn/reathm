# Miscellaneous
*.class # Java class files
*.log # Log files
*.pyc # Python compiled files
*.swp # Vim swap files
.DS_Store # macOS folder metadata

# Editor-specific folders
.atom/ # Atom editor project files
.build/ # General build output directory
.buildlog/ # Build log directory
.history # VS Code local history extension
.svn/ # Subversion version control metadata
.swiftpm/ # Swift Package Manager files
migrate_working_dir/ # Migration tool working directory

# IntelliJ related - IDE project files
*.iml
*.ipr
*.iws
.idea/

# The .vscode folder contains launch configuration and tasks you configure in
# VS Code which you may wish to be included in version control, so this line
# is commented out by default.
#.vscode/

# Flutter/Dart/Pub related
**/doc/api/ # Generated API documentation
**/ios/Flutter/.last_build_id # iOS build state file
.dart_tool/ # Dart's package and build cache. Essential to ignore.
.flutter-plugins # Generated file tracking Flutter plugins
.flutter-plugins-dependencies # Generated file tracking plugin dependencies
.pub-cache/ # Global package cache for Pub
.pub/ # Local package cache
/build/ # Main Flutter build output directory

# Symbolication related (for debugging release builds)
app.*.symbols

# Obfuscation related (for de-obfuscating release build stack traces)
app.*.map.json

# Android Studio will place build artifacts here
/android/app/debug/
/android/app/profile/
/android/app/release/

# Custom & Sensitive Files
app_logs.txt # Application-specific log files

# IMPORTANT: Contains Android signing keys. Never commit.
/android/key.properties

# IMPORTANT: Contains Firebase config for Android, including API keys.
# Generated by placing your project's file from the Firebase console.
/android/app/google-services.json

# IMPORTANT: Contains Firebase config for all platforms. Contains sensitive keys.
# This file is generated locally by running `flutterfire configure`.
lib/firebase_options.dart

# Added by Gemini
update_android_icon.sh # Script for updating icons
*.png # Image files, likely for icons or assets
